<div class="container">
	<div class="row-fluid">
		<div class="span12">
			<% @event_list.each do |city,events| %>
			<h2><%= city %></h2>
				<table class="table table-striped">
					<tr>
						<th>Name</th>
						<th>Date</th>
						<th>Seats Filled</th>
						<th>Max Seats</th>
					</tr>
					<% events.each do |event| %>
					  	<tr>
					  		<td><%= event.event_name %></td>
					  		<td><%= event.event_date %></td>
					  	</tr>
				  	<% end %>
				</table>
				
			<% end %>
		</div>
	</div>
</div>
